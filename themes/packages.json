  },
  "Packages config": {
    "kalitheme": {
      "alacritty": "kalitheme/alacritty.toml",
      "tmux": ["kalitheme/.tmux", "kalitheme/.tmux.conf"],
      "picom": "kalitheme/picom.conf",
      "i3": "kalitheme/i3-config",
      "i3lock": "kalitheme/i3lock.sh",
      "zsh": "kalitheme/.zshrc",
      "bash": ["kalitheme/.bash_logout", "kalitheme/.bashrc", "kalitheme/.bashrc.original", "kalitheme/.bash_profile"],
      "bash-completion": ["kalitheme/bash.bashrc", "kalitheme/bash_completion", "kalitheme/bash_completion.d/"]
    }
  }
}

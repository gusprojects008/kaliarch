{
  "System packages": {
    "kalitheme": {
      "alacritty": "~/.config/alacritty.toml",
      "tmux": ["~/.tmux", "~/.tmux.conf"],
      "feh": "~/.config/feh/.conf",
      "picom": "~/.config/picom/picom.conf",
      "i3": "~/.config/i3/config",
      "i3lock": "~/.config/i3lock/i3lock.sh",
      "scrot": "~/.config/scrot/config",
      "imagemagick": "~/.config/imagemagick/policy.xml",
      "zsh": "~/.zshrc",
      "zsh-syntax-highlighting": "~/.zsh/zsh-syntax-highlighting.zsh",
      "zsh-autosuggestions": "~/.zsh/zsh-autosuggestions.zsh",
      "bash": ["~/.bash_logout", "~/.bashrc", "~/.bashrc.original", "~/.bash_profile"],
      "bash-completion": ["/etc/bash.bashrc", "/etc/bash_completion", "/etc/bash_completion.d/"],
      "ttf-fira-code": "/usr/share/fonts/TTF/",
      "noto-fonts-emoji": "/usr/share/fonts/noto/"
